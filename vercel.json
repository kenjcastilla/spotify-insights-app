    {
      "builds": [
        {
          "src": "wsgi.py",
          "use": "@vercel/python",
          "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.9"
          }
        },
        {
          "src": "static/(.*)",
          "use": "@vercel/static"
        }
      ],
      "routes": [
        {
          "src": "/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "/(.*)",
          "dest": "wsgi.py"
        }
      ]
    }